<%= will_paginate @users %>

<table>
 <tr>
  <td>First Name</td>
  <td>Surname</td>
  <td>Email</td>
  <td>Admin</td>
 </tr>
<% @users.each do |user| %>
 <tr>
  <td><%= user.first_name %></td>
  <td><%= user.surname %></td>
  <td><%= user.email %></td>
  <td><%= user.admin? ? 'Y' : 'N' %></td>
  <td><%= link_to "Edit", edit_admin_user_path(user) %></td>
 </tr>
<% end %>
</table>

<%= will_paginate @users %>